{
	"extends": ["airbnb", "prettier", "plugin:node/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "warn",
		"no-unused-vars": "warn",
		"no-console": "warn",
		"func-names": "off",
		"no-process-env": "off",
		"no-process-exit": "off",
		"object-shorthand": "off",
		"class-methods-use-this": "warn"
	}
}
